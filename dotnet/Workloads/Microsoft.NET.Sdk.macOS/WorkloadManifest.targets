<Project>
	<Import Project="Sdk.props" Sdk="Microsoft.macOS.Sdk" Condition="'$(TargetPlatformIdentifier)' == 'macOS'" />

	<Import Project="Sdk.props" Sdk="Microsoft.NET.Runtime.MonoTargets.Sdk" Condition="'$(TargetPlatformIdentifier)' == 'macOS'" />
	<Import Project="Sdk.targets" Sdk="Microsoft.NET.Runtime.MonoTargets.Sdk" Condition="'$(TargetPlatformIdentifier)' == 'macOS'" />

	<ItemGroup Condition=" '$(TargetFrameworkIdentifier)' == '.NETCoreApp' and $([MSBuild]::VersionGreaterThanOrEquals($(TargetFrameworkVersion), '6.0')) ">
